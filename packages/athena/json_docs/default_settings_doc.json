	"proxy_tls_http_url": null,
	"proxy_tls_ws_url": null,
	"proxy_cache_enabled": true,
	"regex": {
		"is_grpc_url": {
			"source": "^grpcs?://[\\w-._~]+:\\d{1,5}$",
			"flags": "i",
			"notes": "grpc or grpcs && hostname && port (no basic auth, no path)"
		},
		"is_http_url": {
			"source": "^https?://[\\w-._~]+",
			"flags": "i",
			"notes": "http or https && hostname (port allowed) (no basic auth, no path)"
		},
		"is_http_url_with_auth": {
			"source": "^https?://[^\\s/]+$",
			"flags": "i",
			"notes": "http or https protocol && hostname (port & basic auth allowed) (no path)"
		},
		"only_host_and_port": {
			"source": "^[\\w-._~]+:\\d{1,5}$",
			"flags": "i",
			"notes": "hostname && port (no protocol, no basic auth, no path)"
		},
		"is_url": {
			"source": "^\\w+://[\\w-._~]+:\\d{1,5}$",
			"flags": "i",
			"notes": "any protocol && hostname && port (no basic auth, no path)"
		},
		"is_basic_auth_url": {
			"source": "^\\w+://[\\w-._~@:]+:\\d{1,5}",
			"flags": "i",
			"notes": "any protocol && hostname && port && basic auth allowed && path allowed"
		},
		"invalid_msp_id": {
			"source": "[^a-zA-Z\\d-.]",
			"flags": ""
		},
		"invalid_enroll_characters": {
			"source": "[:\\/<>#{}%`[\\]\\\\^~|\\s\"]",
			"flags": ""
		},
		"starts_with_non_letter": {
			"source": "^[^a-zA-Z]+",
			"flags": ""
		},
		"is_valid_id": {
			"source": "^[a-z][a-z0-9]+$",
			"flags": "",
			"notes": "couchdb needs it to start with a letter, deployer can't do underscores or dashes, must be lower case, can contain numbers"
		}
	},
