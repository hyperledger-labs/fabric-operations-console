{
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.renderWhitespace": "all",
    "editor.detectIndentation": false,
    "editor.roundedSelection": true,
    "editor.renderControlCharacters": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.wordWrapColumn": 190,
    "editor.wordWrap": "wordWrapColumn",
    "editor.mouseWheelScrollSensitivity": 3,
    "editor.smoothScrolling": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
		"**/*.bundle.js": true,
		"**/*.bundle.map": true,
		"**/*.spec.ts": true,
		"**/*jquery*/**": true,
		"**/backup/*": true,
		"**/fabric-sdk-node/*": true,
		"**/node_modules/*": true,
		"**/node_modules2/*": true,
		"**/reports/*": true,
		"**/vendor/*": true
	},
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/main.css": true,
        "**/main.js": true,
        "**/temp/**": true,
        "**/*.min.*": true,
        "**/*-min.*": true,
        "**/access*.log": true,
        ".vscode/*": true,
        "**/debug*.log": true,
        "**/bundle.js": true,
        "**/dist/**": true,
        "_backups/*": true,
        "**/*.chunk.*": true,
        "**/coverage": true,
        "**/dist": true,
        "**/logs": true,
        "**/build/static": true
    },
    "tslint.enable": [
        true,
        true
    ],
    "tslint.autoFixOnSave": [
        false,
        false
    ],
    "tslint.alwaysShowRuleFailuresAsWarnings": false,
    "tslint.alwaysShowStatus": [
        true,
        true
    ],
    "javascript.suggestionActions.enabled": false,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "window.zoomLevel": 0,
    "workbench.editor.enablePreview": false,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "html.format.wrapLineLength": 170,
    "telemetry.enableTelemetry": false,
    "typescript.validate.enable": true,
    // "typescript.tsdk": [
    //     "./node_modules/typescript/lib",
    //     "./stitch/node_modules/typescript/lib"
    // ],
    "cSpell.enabledLanguageIds": [
        "c",
        "cpp",
        "csharp",
        "go",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "markdown",
        "php",
        "plaintext",
        "python",
        "text",
        "typescript",
        "typescriptreact",
        "yml",
        "c",
        "cpp",
        "csharp",
        "go",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "latex",
        "markdown",
        "php",
        "plaintext",
        "python",
        "text",
        "typescript",
        "typescriptreact",
        "yml"
    ],
    "cSpell.dictionaryDefinitions": [
        {
            "name": "blockchainTerms",
            "path": "./dictionary_blockchain.txt"
        }
    ],
    "cSpell.dictionaries": [
        "blockchainTerms"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "search.useIgnoreFiles": false
}
